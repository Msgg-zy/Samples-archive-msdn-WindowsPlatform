<Page
	x:Class="SDKTemplateCS.ScenarioOutput2"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	d:DesignHeight="462"
	d:DesignWidth="841">

    <StackPanel Margin="0,0,-200,0">
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="Policy Name " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="Requested Value " VerticalAlignment="Center" Width="642" Canvas.Left="280" Canvas.Top="10"/>
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="Compliance Result " VerticalAlignment="Center" Width="642" Canvas.Left="480" Canvas.Top="10"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
        	<Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
        		<TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="RequireEncryption (True/False) " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
        		<TextBox x:Name="RequireEncryptionResult" Width="160" Height="30" Canvas.Left="480" />
                <CheckBox x:Name="RequireEncryptionValue" HorizontalAlignment="Left" VerticalAlignment="Center"  Width="36" Canvas.Left="294" Canvas.Top="-9" Margin="10,0,0,0"/>
        	</Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0" >
        		<TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="MinPasswordLength (0-16) : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
        		<TextBox x:Name="MinPasswordLengthValue" Width="100" Height="30" Canvas.Left="280"/>
                <TextBox x:Name="MinPasswordLengthResult" Width="160" Height="30" Canvas.Left="480" />
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="DisallowConvenienceLogon (True/False) : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="DisallowConvenienceLogonResult" Width="160" Height="30" Canvas.Left="480" />
                <CheckBox x:Name="DisallowConvenienceLogonValue" HorizontalAlignment="Left" VerticalAlignment="Center"  Width="36" Canvas.Left="294" Canvas.Top="-9" Margin="10,0,0,0"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="MinPasswordComplexCharacters (0-4) : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="MinPasswordComplexCharactersValue" Width="100" Height="30" Canvas.Left="280"/>
                <TextBox x:Name="MinPasswordComplexCharactersResult" Width="160" Height="30" Canvas.Left="480" />
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="PasswordExpiration (0-999 days): " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="PasswordExpirationValue" Width="100" Height="30" Canvas.Left="280"/>
                <TextBox x:Name="PasswordExpirationResult" Width="160" Height="30" Canvas.Left="480" />
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="PasswordHistory (0-24) : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="PasswordHistoryValue" Width="100" Height="30" Canvas.Left="280"/>
                <TextBox x:Name="PasswordHistoryResult" Width="160" Height="30" Canvas.Left="480" />
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="MaxPasswordFailedAttempts (0, 4-999): " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="MaxPasswordFailedAttemptsValue" Width="100" Height="30" Canvas.Left="280" />
                <TextBox x:Name="MaxPasswordFailedAttemptsResult" Width="160" Height="30" Canvas.Left="480" />
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="MaxInactivityTimeLock (0-599940 secs): " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="MaxInactivityTimeLockValue" Width="100" Height="30" Canvas.Left="280"/>
                <TextBox x:Name="MaxInactivityTimeLockResult" Width="160" Height="30" Canvas.Left="480" />
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="Error: " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="Scenario2DebugArea" HorizontalAlignment="Left" Height="15" Margin="10,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="612" Canvas.Left="20" Canvas.Top="5"/>
            </Canvas>
        </StackPanel>
    </StackPanel>
</Page>