<Page
	x:Class="CertificateEnrollment.ScenarioInput2"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	d:DesignHeight="300"
	d:DesignWidth="400">

	<!-- Replace the markup below with your specific markup. -->
    <StackPanel x:Name="Scenario2Input">
        <TextBlock Style="{StaticResource DescriptionTextStyle}" TextWrapping="Wrap" Text="Import a certificate in PFX format" />
        <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
            <Button x:Name="ImportPfx" Content="Import" Margin="0,0,10,0" Click="ImportPfx_Click"/>
        </StackPanel>
    </StackPanel>

</Page>