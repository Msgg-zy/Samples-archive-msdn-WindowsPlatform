<Page
	x:Class="SDKTemplateCS.ScenarioOutput1"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	d:DesignHeight="381"
	d:DesignWidth="700">

    <!-- Replace the markup below with your specific markup. -->
    <StackPanel Margin="0,0,-225,0">
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="Device ID :" VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="DeviceID" Width="280" Height="30" Canvas.Left="150"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="OperatingSystem : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="OperatingSystem" Width="280" Height="30" Canvas.Left="150"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="FriendlyName : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="FriendlyName" Width="280" Height="30" Canvas.Left="150"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="SystemManufacturer : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="SystemManufacturer" Width="280" Height="30" Canvas.Left="150"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="SystemProductName : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="SystemProductName" Width="280" Height="30" Canvas.Left="150"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="SystemSku : " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="SystemSku" Width="280" Height="30" Canvas.Left="150"/>
            </Canvas>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Canvas VerticalAlignment="Center" Height="40" Margin="10,0,0,0">
                <TextBlock TextWrapping="Wrap" Style="{StaticResource DescriptionTextStyle}" Text="Error: " VerticalAlignment="Center" Width="642" Canvas.Left="-10" Canvas.Top="10"/>
                <TextBox x:Name="Scenario2DebugArea" HorizontalAlignment="Left" Height="15" Margin="10,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="612" Canvas.Left="20" Canvas.Top="5"/>
            </Canvas>
        </StackPanel>
    </StackPanel>
</Page>